<h1>Log in as caterer</h1>
<br>

<br>
<div class="">
  <%= form_tag(:action => "caterer_login", :method => "post") do %>
  <p>
    <%= label_tag(:username) %>:
    <%= text_field_tag(:username) %>
  </p>
  <p>
    <%= label_tag(:password) %>:
    <%= password_field_tag(:password) %>
  </p>
    <%= submit_tag("Login") %>
  <% end %>
</div>

<br>
<br>


<%=link_to('Back', login_path)%>